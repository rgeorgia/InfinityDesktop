DestroyMenu MyMenuApplications
AddToMenu MyMenuApplications "My Applications" Title
+ "" Nop
+ "[Network]" nop
+ "Thunderbird" Exec exec thunderbird %u
+ "Chromium" Exec exec chrome %U
+ "FileZilla" Exec exec filezilla
+ "Telegram Desktop" Exec exec telegram-desktop -- %u
+ "Web Browser" Exec exec exo-open --launch WebBrowser %u
+ "Mail Reader" Exec exec exo-open --launch MailReader %u
+ "" nop
+ "[System]" nop
+ "Gigolo" Exec exec gigolo
+ "XTerm" Exec exec xterm
+ Htop Exec exec uxterm htop
+ "" nop
+ "[Graphics]" nop
+ "Lumina Screenshot" Exec exec lumina-screenshot -new-instance
+ "Blender" Exec exec blender %f
+ "Inkscape" Exec exec inkscape
+ "Ristretto" Exec exec ristretto %F
+ "Feh" Exec exec feh --start-at %u
+ "GIMP" Exec exec gimp-2.10 %U
+ "" nop
+ "[Utility]" nop
+ Vim Exec exec uxterm vim %F
+ "xfburn" Exec exec xfburn -i %f
+ "Terminal Emulator" Exec exec exo-open --launch TerminalEmulator
+ Neovim Exec exec uxterm nvim %F
+ "Character Map" Exec exec gucharmap
+ "Shortcut Editor" Exec exec lxshortcut -i %f
+ "Mousepad" Exec exec mousepad %F
+ "GVim" Exec exec gvim -f %F
+ "File Manager" Exec exec exo-open --launch FileManager %u
+ "Leafpad" Exec exec leafpad %f
+ "" nop
+ "[FileManager]" nop
+ "pcmanfm" Exec exec pcmanfm %U
+ "" nop
+ "[Development]" nop
+ "GoLand" Exec exec /usr/local/share/goland/bin/goland.sh
+ "Icon Browser" Exec exec gtk3-icon-browser
+ "Widget Factory" Exec exec gtk3-widget-factory
+ "PyCharm Pro" Exec exec /usr/local/share/pycharm-pro/bin/pycharm.sh
+ "bluefish" Exec exec bluefish %F
+ "" nop
+ "[Settings]" nop
+ "Preferred Applications" Exec exec exo-preferred-applications
+ "XScreenSaver Stop" Exec exec /usr/local/bin/xscreensaver-command -exit
+ "XScreenSaver Blank" Exec exec /usr/local/bin/xscreensaver-command -activate
+ "XScreenSaver Start" Exec exec /usr/local/bin/xscreensaver
+ "XScreenSaver Lock" Exec exec /usr/local/bin/xscreensaver-command -lock
+ "" nop
