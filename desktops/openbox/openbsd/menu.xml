<?xml version="1.0" encoding="UTF-8"?>

<openbox_menu xmlns="http://openbox.org/3.4/menu">

<menu id="apps-accessories-menu" label="Accessories">
</menu>

<menu id="apps-editors-menu" label="Editors">
    <item label="Leafpad" icon="/usr/local/share/icons/Moka/32x32/apps/leafpad.png">
    <action name="Execute">
      <command>leafpad</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="Mousepad" icon="/usr/local/share/icons/hicolor/48x48/apps/org.xfce.mousepad.png">
    <action name="Execute">
      <command>mousepad</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
  <item label="PycharmCE 2022.1" icon="/usr/local/pycharm/bin/pycharm.png">
    <action name="Execute">
        <command>/usr/local/bin/pycharm</command>
      <startupnotify>
        <enabled>yes</enabled>
      </startupnotify>
    </action>
  </item>
</menu>

<menu id="apps-term-menu" label="Terminals">
    <item label="Sakura" icon="/usr/local/share/icons/Moka/32x32/apps/sakura.png">
